<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.wlq.intentapplication.FActivity">
    <TextView
        android:id="@+id/text1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="这是第一个页面"/>
    <Button
        android:id="@+id/button1_f"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="第一种启动方式"
        android:layout_below="@+id/text1"
        android:layout_marginTop="20dp"/>

    <Button
        android:id="@+id/button2_s"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="第二种启动方式"
        android:layout_below="@+id/button1_f"
        android:layout_marginTop="20dp"/>

    <TextView
        android:id="@+id/text2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button2_s"
        android:layout_marginTop="30dp"
        android:text="把第二个页面回传的数据显示出来"
        android:textSize="20dp"/>

</RelativeLayout>
